{
  "options": {
    "report": "true",
    "clean": "true",
    "destPrefix": "src/assets/lib"
  },
  "bootstrap": {
    "options":{
      "destPrefix": "src/"
    },
    "files":{
      "less/includes/bootstrap" : "bootstrap/less",
      "assets/fonts/bootstrap" : "bootstrap/fonts",
      "assets/js/bootstrap" : "bootstrap/dist/js"
    }
  },
  "jquery":{
    "options":{
      "destPrefix": "src/assets/js/jquery"
    },
    "files":{
      "/" : "jquery/dist"
    }
  },
  "font-awesome":{
    "options":{
      "destPrefix": "src/assets/fonts/font-awesome"
    },
    "files":{
      "css" : "font-awesome/css",
      "fonts" : "font-awesome/fonts"
    }
  }
}